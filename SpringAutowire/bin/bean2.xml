<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->

<bean id="s" class="com.cjc.constructor.Student" autowire="constructor" >
	<constructor-arg name="rollno" value="1"></constructor-arg>
	<constructor-arg name="name" value="Chetan Patil"></constructor-arg>
	
</bean>

<bean id = "add" class="com.cjc.constructor.Address">
<constructor-arg name="padd" value="Pune"></constructor-arg>
<constructor-arg name="ladd" value="Solapur"></constructor-arg>
</bean>

</beans>