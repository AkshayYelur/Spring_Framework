<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->

<bean id="s" class="com.cjc.constructorbased.objecttype.Student1" >
<constructor-arg name="rollno" value="1"></constructor-arg>	
<constructor-arg name="name" value="CJC"></constructor-arg>
<constructor-arg name="addr" ref="add"></constructor-arg>
</bean>
<bean id="add" class="com.cjc.constructorbased.objecttype.Address1">
<constructor-arg name="padd" value="karve nagar"></constructor-arg>
<constructor-arg name="ladd" value="Pune"></constructor-arg>
</bean>

</beans>